{
  "project": {
    "version": "1.0.0-beta.19-3",
    "name": "ant-ultima-cli"
  },
  "apps": [{
    "root": "src",
    "outDir": "dist",
    "assets": [
      "assets",
      "favicon.ico"
    ],
    "index": "index.html",
    "main": "main.ts",
    "test": "test.ts",
    "tsconfig": "tsconfig.json",
    "prefix": "app",
    "mobile": false,
    "styles": [
      "assets/layout/css/animate.css",
      "assets/demo/css/fullcalendar.css",
      "../node_modules/bootstrap/dist/css/bootstrap.css",
      "../node_modules/primeng/resources/primeng.min.css",
      "../node_modules/slick-carousel/slick/slick.css",
      "../node_modules/slick-carousel/slick/slick-theme.css",
      "styles.scss"
    ],
    "scripts": [
      "assets/demo/js/jquery.deps.js",
      "assets/layout/js/layout.js",
      "assets/demo/js/charts.min.js",
      "assets/demo/js/moment.js",
      "assets/demo/js/fullcalendar.js",
      "assets/demo/js/quill.min.js",
      "../node_modules/slick-carousel/slick/slick.min.js",
      "../node_modules/socket.io-client/dist/socket.io.min.js"
    ],
    "environments": {
      "source": "environments/environment.ts",
      "dev": "environments/environment.ts",
      "prod": "environments/environment.prod.ts"
    }
  }],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": false,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": false
    }
  }
}
